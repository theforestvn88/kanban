<% if @result.success %>
    <% if @result.after_list %>
        <%= turbo_stream.move_before(dom_id(@result.list), before: dom_id(@result.after_list)) %>
    <% elsif @result.before_list %>
        <%= turbo_stream.move_before(dom_id(@result.before_list), before: dom_id(@result.list)) %>
    <% end %>
<% else %>
<% end %>
